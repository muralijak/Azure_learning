{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-demo-1984"
		},
		"AzureDataLakeStorage_if_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage_if'"
		},
		"ls_AzureBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_AzureBlobStorage'"
		},
		"ls_AzureDataLakeStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_AzureDataLakeStorage'"
		},
		"ls_AzureSqlDatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_AzureSqlDatabase'"
		},
		"ls_AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_AzureSqlDatabase1'"
		},
		"ls_adls_adfdemodl1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_adfdemodl1'"
		},
		"AzureDataLakeStorage_if_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adfdemodl13.dfs.core.windows.net/"
		},
		"ls_AzureDataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adfdemodl13.dfs.core.windows.net/"
		},
		"ls_RestAPI_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "http://dummy.restapiexample.com/api/v1/employees"
		},
		"ls_adls_adfdemodl1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adfdemodl13.dfs.core.windows.net/"
		},
		"trigger_event_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/d66a18b1-3440-4ce5-9de2-91d6d03f549d/resourceGroups/ADF-demo-RG/providers/Microsoft.Storage/storageAccounts/adfdemosa13"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-demo-1984_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage_if_accountKey": {
						"value": "[parameters('AzureDataLakeStorage_if_accountKey')]"
					},
					"ls_AzureBlobStorage_connectionString": {
						"value": "[parameters('ls_AzureBlobStorage_connectionString')]"
					},
					"ls_AzureDataLakeStorage_accountKey": {
						"value": "[parameters('ls_AzureDataLakeStorage_accountKey')]"
					},
					"ls_AzureSqlDatabase_connectionString": {
						"value": "[parameters('ls_AzureSqlDatabase_connectionString')]"
					},
					"ls_AzureSqlDatabase1_connectionString": {
						"value": "[parameters('ls_AzureSqlDatabase1_connectionString')]"
					},
					"ls_adls_adfdemodl1_accountKey": {
						"value": "[parameters('ls_adls_adfdemodl1_accountKey')]"
					},
					"AzureDataLakeStorage_if_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage_if_properties_typeProperties_url')]"
					},
					"ls_AzureDataLakeStorage_properties_typeProperties_url": {
						"value": "[parameters('ls_AzureDataLakeStorage_properties_typeProperties_url')]"
					},
					"ls_RestAPI_properties_typeProperties_url": {
						"value": "[parameters('ls_RestAPI_properties_typeProperties_url')]"
					},
					"ls_adls_adfdemodl1_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_adfdemodl1_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-demo-1984_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"trigger_event_properties_typeProperties_scope": {
						"value": "[parameters('trigger_event_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-demo-1984_ArmTemplate_0')]"
			]
		},
		{
			"name": "adf-demo-1984_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-demo-1984_ArmTemplate_1')]"
			]
		}
	]
}